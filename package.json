{
  "name": "react-cookie-consent-notification",
  "version": "1.1.1",
  "description": "The package will help you display a notification of consent to the use of cookies on the site",
  "main": "./dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint --report-unused-disable-directives src",
    "fix": "eslint --fix --report-unused-disable-directives src",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "setup": "cp ./.github/hooks/pre-commit ./.git/hooks/pre-commit && chmod +x ./.git/hooks/pre-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awibox/react-cookie-consent-notification.git"
  },
  "keywords": [
    "react-component",
    "cookie-consent",
    "npm-package",
    "notification"
  ],
  "author": "awibox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awibox/react-cookie-consent-notification/issues"
  },
  "homepage": "https://github.com/awibox/react-cookie-consent-notification#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.2.2",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^26.0.1",
    "jest-enzyme": "^7.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "combo-storage": "^2.0.3",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/jest-setup-tests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
